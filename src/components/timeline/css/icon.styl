.tl-icon-hidden:before
  content: "\e904"

.tl-icon-lock:before
  content: "\e905"

.tl-icon-visible:before
  content: "\e906"

.tl-icon-unlock:before
  content: "\e907"

.tl-icon-first:before
  content: "\e908"

.tl-icon-indicator:before

@font-face
  font-family: 'timeline'
  src:  url('fonts/timeline.eot?a6ogxb')
  src:  url('fonts/timeline.eot?a6ogxb#iefix') format('embedded-opentype'),
    url('fonts/timeline.ttf?a6ogxb') format('truetype'),
    url('fonts/timeline.woff?a6ogxb') format('woff'),
    url('fonts/timeline.svg?a6ogxb#timeline') format('svg')
  font-weight: normal
  font-style: normal

[class^="tl-"], [class*=" tl-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'timeline' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  display: inline-block

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.tl-icon-close:before
  content: "\e900"

.tl-icon-prev:before
  content: "\e901"

.tl-icon-download:before
  content: "\e902"

.tl-icon-next:before
